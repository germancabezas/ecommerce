


<div class="container">
	<div class="row">
		<div class="col-md-6 col-md-offset-3">
			<h1> Add more products </h1>
			<%=form_tag'/products' do%>
				<%=label_tag :title%>
				<%=text_field_tag :title%>
				<%=label_tag :image%>
				<%=text_field_tag :image%>
	 
				<%=label_tag :price%>
				<%=text_field_tag :price%>

				<%=label_tag :description%>
				<%=text_field_tag :description%>

				<%=label_tag :category%>
				<%=text_field_tag :category%>

				<%=label_tag :brand%>
				<%=text_field_tag :brand%>

				<%=submit_tag "Add"%>
			<%end%>
		</div>
	</div>
</div>